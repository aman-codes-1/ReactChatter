{
  "name": "reactchatter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "react-scripts build",
    "build:server": "cd server && npm install && npm run build",
    "build:both": "npm run build && npm run build:server",
    "start": "react-scripts start",
    "start:server": "cd server && npm run start:dev",
    "start:server:prod": "cd server && npm run start:prod",
    "start:prod": "npm run start:server:prod",
    "lint": "eslint \"{src,apps,libs,test}/**/*.{js,jsx,ts,tsx}\"",
    "lint:fix": "npm run lint -- --fix",
    "format": "prettier --write \"{src,apps,libs,test}/**/*.{js,jsx,ts,tsx}\"",
    "format:check": "prettier --check \"{src,apps,libs,test}/**/*.{js,jsx,ts,tsx}\"",
    "compile": "graphql-codegen --require dotenv/config --config codegen.ts",
    "watch": "graphql-codegen --require dotenv/config --config codegen.ts -w",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@apollo/client": "^3.10.5",
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.5",
    "@mui/icons-material": "^5.15.20",
    "@mui/lab": "^5.0.0-alpha.158",
    "@mui/material": "^5.15.20",
    "@mui/system": "^5.15.20",
    "@parcel/watcher": "^2.4.1",
    "@react-oauth/google": "^0.12.1",
    "@testing-library/user-event": "^14.5.2",
    "axios": "^1.7.2",
    "dotenv": "^16.4.5",
    "graphql-ws": "^5.16.0",
    "jwt-decode": "^4.0.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.23.1",
    "react-scripts": "5.0.1",
    "socket.io-client": "^4.7.5",
    "subscriptions-transport-ws": "^0.11.0",
    "web-vitals": "^3.5.2",
    "which": "^4.0.0"
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@babel/plugin-transform-runtime": "^7.24.7",
    "@babel/preset-env": "^7.24.7",
    "@babel/preset-react": "^7.24.7",
    "@graphql-codegen/cli": "^5.0.2",
    "@graphql-codegen/client-preset": "^4.2.6",
    "@graphql-typed-document-node/core": "^3.2.0",
    "@testing-library/jest-dom": "^6.4.6",
    "@testing-library/react": "^14.3.1",
    "@testing-library/user-event": "14.5.2",
    "@types/crypto-js": "^4.2.2",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.14.2",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "babel-plugin-dynamic-import-node": "2.3.3",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "3.6.1",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.2",
    "typescript": "^4.9.5"
  }
}
