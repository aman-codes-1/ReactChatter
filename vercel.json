{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://reactchatter-server-production.up.railway.app/:path*"
    },
    {
      "source": "/api/:path*",
      "destination": "https://reactchatter-server-production.up.railway.app/:path*/"
    }
  ]
}
